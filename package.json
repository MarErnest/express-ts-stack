{
  "name": "express-ts-stack",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Mark Ernest R. Matute",
  "description": "TypeScript, Express, Starter",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-winston": "^3.2.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "rate-limiter-flexible": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.18",
    "winston": "^3.2.1"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "tsc && jest --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "db:seed": "node dist/scripts/initialSeed.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-winston": "^3.0.3",
    "@types/helmet": "^0.0.43",
    "@types/node": "^12.6.2",
    "@types/jest": "^24.0.11",
    "@types/supertest": "^2.0.7",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "tslint": "^5.18.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2"
  }
}
